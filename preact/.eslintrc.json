{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["import", "react"],
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "react/prop-types": "off"
  }
}
